
<% include ./partials/header %>

<form action="index.html" method="post">

    <h1 class="heading">Contact Us</h1>

    <form action="" method="POST">
        <fieldset class="all_forms">
            <label for="name">Name:</label><br>
            <input type="text" id="name" name="user_name"><br>
            <label for="mail">Email:</label><br>
            <input type="email" id="mail" name="user_email"><br>
            <label for="phone">Phone:</label><br>
            <input type="phone" id="phone" name="user_phone"><br>
            <label for="comments">Questions or Comments:</label><br>
            <textarea id="comments" name="comments"></textarea><br>
        </fieldset>

        <button type="submit" class="heading">Submit</button>
    </form>
</form>

